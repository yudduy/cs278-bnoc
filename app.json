{
  "expo": {
    "name": "Daily Meetup Selfie",
    "slug": "bnoc-react",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#1A1A1A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bnoc.app",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Daily Meetup Selfie to access your camera to take selfies with friends.",
        "NSMicrophoneUsageDescription": "Allow Daily Meetup Selfie to access your microphone for video calls.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/app-icon.png",
        "backgroundColor": "#1A1A1A"
      },
      "package": "com.bnoc.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {},
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Daily Meetup Selfie to access your camera to take selfies with friends."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Daily Meetup Selfie to access your photos to select profile images."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#990000",
          "sounds": [
            "./assets/notification-sound.wav"
          ]
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "scheme": "dailymeetup",
    "primaryColor": "#990000",
    "extra": {
      "eas": {
        "projectId": "fe2e5eb2-c438-44c4-874f-0066d2a2643a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fe2e5eb2-c438-44c4-874f-0066d2a2643a"
    }
  }
}
